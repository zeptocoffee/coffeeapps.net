{
  "name": "coffeeapps.net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npx @11ty/eleventy --serve & npx tailwindcss -i styles/tailwind.css -c ./tailwind.config.js -o _site/tailwind.min.css --watch",
    "build": "ELEVENTY_ENV=production npx @11ty/eleventy & npx tailwindcss -i styles/tailwind.css -c ./tailwind.config.js -o _site/tailwind.min.css --minify"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {}
}
